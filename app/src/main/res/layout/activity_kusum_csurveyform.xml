                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="@string/typesofirrigation"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_12sdp" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edt_bg_shape"
                    android:layout_marginTop="@dimen/_5sdp">
                <Spinner
                    android:id="@+id/typesOfIrrigationSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/spinner_bg"
                    android:padding="@dimen/_10sdp"
                    android:entries="@array/irrigationInstalled"/>
                </RelativeLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="@string/isSouthFacingShadow"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_12sdp" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edt_bg_shape"
                    android:layout_marginTop="@dimen/_5sdp">
                <Spinner
                    android:id="@+id/southfacingShadowSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/spinner_bg"
                    android:padding="@dimen/_10sdp"
                    android:entries="@array/yesNoSpinner"/>

                </RelativeLayout>

            </LinearLayout>


            <TextView
